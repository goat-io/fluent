{
  "name": "@goatlab/formio-utils",
  "version": "0.7.1",
  "description": "Readable query Interface & API generator for TS and Node",
  "scripts": {
    "install-p": "yarn",
    "prebuild": "rimraf dist",
    "prestart": "rimraf dist",
    "predev": "rimraf dist",
    "test": "jest --forceExit --passWithNoTests",
    "dev": "tsc --watch",
    "lint": "eslint src --fix",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "update:packages": "npm-check-updates -u && yarn",
    "serve": "tsc -w",
    "build": "tsc"
  },
  "dependencies": {
    "@goatlab/fluent": "*",
    "@goatlab/node-utils": "*",
    "bson": "^4.6.2",
    "delete-property": "^0.0.4",
    "formiojs": "^4.14.5",
    "handlebars": "^4.7.7",
    "joi": "14.3.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.4.6",
    "path": "^0.12.7",
    "q": "^1.5.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.5.0",
    "jest": "^27.5.1",
    "jest-environment-node": "^27.5.1",
    "prettier": "^2.6.1",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "turbo": "^1.1.10",
    "typescript": "^4.6.3"
  },
  "keywords": [
    "typescript",
    "TS"
  ],
  "author": "ignacio.cabrera@goatlab.io",
  "license": "MIT",
  "homepage": "https://docs.goatlab.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "./dist/**/*"
  ],
  "engines": {
    "node": ">=14.16.0"
  }
}
