    "@types/node": "^17.0.23",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.5.0",
    "jest": "catalog:",
    "jest-environment-node": "^27.5.1",
    "prettier": "^2.6.1",
    "pretty-quick": "^3.1.3",
    "ts-jest": "catalog:",
    "ts-node": "catalog:",
    "turbo": "^1.1.10",
    "typescript": "^4.6.3"
  },
  "files": [
    "dist"
