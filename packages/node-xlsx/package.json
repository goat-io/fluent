    "@goatlab/tsconfig": "workspace:*",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.9",
    "dotenv-cli": "^7.1.0",
    "eslint": "^8.36.0",
    "jest": "catalog:",
    "jest-environment-node": "^29.5.0",
    "prettier": "^2.8.7",
    "pretty-quick": "^3.1.3",
    "ts-jest": "catalog:",
    "ts-node": "catalog:",
    "turbo": "^1.1.10",
    "typescript": "^5.0.2"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://docs.goatlab.io",
