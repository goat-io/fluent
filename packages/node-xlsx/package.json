  "main": "./dist/index.js",
  "version": "0.0.2",
  "name": "@goatlab/node-xlsx",
  "description": "Efficiently work with xlsx files streams in Node.js",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint src --fix",
    "prebuild": "rimraf dist",
    "predev": "rimraf dist",
    "prestart": "rimraf dist",
    "serve": "tsc -w",
    "test": "jest --forceExit --passWithNoTests",
    "update:packages": "npm-check-updates -u && yarn"
  },
  "dependencies": {
    "@goatlab/js-utils": "workspace:*"
