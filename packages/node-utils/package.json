{
  "name": "@goatlab/node-utils",
  "version": "0.8.21",
  "description": "Readable query Interface & API generator for TS and Node",
  "homepage": "https://docs.goatlab.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "install-p": "yarn",
    "prebuild": "rimraf dist",
    "prestart": "rimraf dist",
    "predev": "rimraf dist",
    "test": "jest --forceExit",
    "dev": "tsc --watch",
    "lint": "eslint src --fix",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "update:packages": "npm-check-updates -u && yarn",
    "serve": "tsc -w",
    "build": "tsc"
  },
  "keywords": [
    "typescript",
    "TS"
  ],
  "author": "ignacio.cabrera@goatlab.io",
  "license": "MIT",
  "dependencies": {
    "@goatlab/js-utils": "^0.8.26",
    "@types/through2-concurrent": "^2.0.1",
    "bcrypt": "^5.0.1",
    "child_process": "^1.0.2",
    "cross-spawn": "^7.0.3",
    "detect-port": "^1.3.0",
    "dockerode": "^3.3.1",
    "fs": "^0.0.1-security",
    "got": "11.0.1",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "modern-spawn": "^1.0.0",
    "net": "^1.0.2",
    "path": "^0.12.7",
    "portfinder": "^1.0.32",
    "rimraf": "^3.0.2",
    "through2-concurrent": "^2.0.0",
    "verror": "^1.10.1",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.5.0",
    "jest": "^27.5.1",
    "jest-environment-node": "^27.5.1",
    "prettier": "^2.6.1",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "turbo": "^1.1.10",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=14.16.0"
  }
}
