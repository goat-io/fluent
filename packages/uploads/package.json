{
  "name": "@goatlab/uploads",
  "version": "0.7.2",
  "description": "Express upload helpers",
  "homepage": "https://docs.goatlab.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "./dist/**/*"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "prestart": "rimraf dist",
    "predev": "rimraf dist",
    "test": "jest --forceExit --passWithNoTests",
    "dev": "tsc --watch",
    "lint": "eslint src --fix",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "update:packages": "npm-check-updates -u && yarn",
    "serve": "tsc -w",
    "build": "tsc"
  },
  "keywords": [
    "typescript",
    "TS"
  ],
  "author": "ignacio.cabrera@goatlab.io",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1105.0",
    "fs": "^0.0.1-security",
    "multer": "^2.0.1",
    "multer-azure-blob-storage": "^1.1.0",
    "multer-google-storage": "^1.3.0",
    "multer-s3": "^2.10.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@goatlab/tsconfig": "workspace:*",
    "@types/jest": "^27.4.1",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^2.7.11",
    "@types/node": "^17.0.23",
    "auto-changelog": "^2.4.0",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.5.0",
    "husky": "^7.0.4",
    "jest": "catalog:",
    "jest-environment-node": "^27.5.1",
    "npm-pack-zip": "^1.3.0",
    "prettier": "^2.6.1",
    "pretty-quick": "^3.1.3",
    "release-it": "^14.13.1",
    "ts-jest": "catalog:",
    "ts-node": "catalog:",
    "turbo": "^1.1.10",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=14.16.0"
  }
}
