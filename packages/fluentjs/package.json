    "prebuild": "rimraf dist",
    "prestart": "rimraf dist",
    "predev": "rimraf dist",
    "test": "jest --forceExit --passWithNoTests",
    "dev": "tsc --watch",
    "lint": "eslint src --fix",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "update:packages": "npm-check-updates -u && yarn",
    "serve": "tsc -w",
    "build": "tsc"
  },
