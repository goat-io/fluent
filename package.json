{
  "name": "@goatlab/fluent",
  "version": "0.5.3",
  "description": "Readable query Interface & API generator for TS and Node",
  "homepage": "https://docs.goatlab.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "prebuild": "npx rimraf dist",
    "prestart": "npx rimraf dist",
    "predev": "npx rimraf dist",
    "test:local": "npx jest --forceExit",
    "test": "npx jest --forceExit --detectOpenHandles",
    "dev": "npx tsc --watch",
    "tslint-check": "npx tslint-config-prettier-check ./tslint.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "update:packages": "ncu -u && npm install",
    "serve": "tsc -w",
    "build": "npx tsc",
    "pack": "npx npm-pack-zip",
    "release": "release-it patch --no-npm.publish --ci",
    "release:patch": "release-it patch --no-npm.publish --ci",
    "release:minor": "release-it minor --no-npm.publish --ci",
    "release:major": "release-it major --no-npm.publish --ci ",
    "rc": "release-it --no-npm.publish --ci --preRelease=rc",
    "rc:patch": "release-it patch --no-npm.publish --ci --preRelease=rc",
    "rc:minor": "release-it minor --no-npm.publish --ci --preRelease=rc",
    "rc:major": "release-it major --no-npm.publish --ci --preRelease=rc",
    "changelog": "auto-changelog --template changelog-template.hbs -p -u --commit-limit false",
    "changelog-debug": "auto-changelog --template changelog-template.hbs -p --template json --output changelog-data.json"
  },
  "keywords": [
    "goat",
    "fluent",
    "loopback",
    "nestjs",
    "graphql",
    "javascript",
    "typescript",
    "TS"
  ],
  "author": "ignacio.cabrera@goatlab.io",
  "license": "MIT",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        2,
        "never",
        []
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm test",
      "commit-msg": "exec < /dev/tty && git cz --hook || true && commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run build"
    }
  },
  "release-it": {
    "src": {
      "commitMessage": "ci: CI-1 [Release] v%s"
    },
    "git": {
      "requireBranch": "master",
      "commitArgs": [
        "--no-verify"
      ]
    },
    "hooks": {
      "before:init": [
        "npm run build",
        "npm run pack"
      ],
      "after:bump": "npm run changelog",
      "after:release": "echo Successfully released ${name} v${version} to ${repo.repository}."
    },
    "github": {
      "release": true,
      "assets": [
        "./*.zip"
      ]
    }
  },
  "auto-changelog": {
    "commitLimit": false,
    "unreleased": true,
    "issueUrl": "https://goatlab.atlassian.net/browse/{id}",
    "replaceText": {
      "[Ff]eature:": "",
      "[Ff]ix:": "",
      "[Bb]reak:": "",
      "([A-Z]+-\\d+)": "[$1](https://goatlab.atlassian.net/browse//$1) - "
    },
    "includeBranch": [
      "master"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira"
    },
    "mongodbMemoryServer": {
      "version": "latest"
    }
  },
  "dependencies": {
    "@azure/storage-blob": "^12.5.0",
    "@nestjs/common": "^8.0.7",
    "@nestjs/core": "^7.6.15",
    "@nestjs/graphql": "^7.10.6",
    "@nestjs/swagger": "^4.8.0",
    "@react-native-async-storage/async-storage": "^1.15.4",
    "@stamp/compose": "^1.0.2",
    "@stamp/it": "^1.1.0",
    "@stamp/privatize": "^1.0.3",
    "agenda": "^4.1.3",
    "amqplib": "^0.7.1",
    "await-to-js": "^3.0.0",
    "aws-sdk": "^2.903.0",
    "axios": "^0.21.1",
    "axios-logger": "^2.5.0",
    "axios-retry": "^3.1.9",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "bson": "^4.3.0",
    "btoa": "^1.2.1",
    "bull": "^3.22.4",
    "bullmq": "^1.24.4",
    "chalk": "^4.1.1",
    "child_process": "^1.0.2",
    "class-validator": "^0.13.1",
    "cli-progress": "^3.9.0",
    "cluster": "^0.7.7",
    "colors": "^1.4.0",
    "commander": "^7.2.0",
    "cron": "^1.8.2",
    "dayjs": "^1.10.4",
    "deep-diff": "^1.0.2",
    "delete-property": "0.0.4",
    "dockerode": "^3.3.0",
    "faker": "^5.5.3",
    "figlet": "^1.5.0",
    "firebase-admin": "^9.7.0",
    "fireorm": "^0.20.0",
    "formiojs": "^4.13.1",
    "got": "^11.8.2",
    "graphql": "^15.5.0",
    "handlebars": "^4.7.7",
    "http-errors": "^1.8.0",
    "inquirer": "^8.0.0",
    "ioredis": "^4.27.2",
    "joi": "14.3.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "loglevel": "^1.7.1",
    "loglevel-plugin-prefix": "^0.8.4",
    "loki-react-native-asyncstorage-adapter": "^1.0.1",
    "lokijs": "^1.5.12",
    "md5": "^2.3.0",
    "memory-cache": "^0.2.0",
    "moment": "^2.29.1",
    "mongoose": "^5.12.7",
    "multer": "^1.4.2",
    "multer-azure-blob-storage": "^1.0.2",
    "multer-google-storage": "^1.3.0",
    "multer-s3": "^2.9.0",
    "nanoid": "^3.1.22",
    "nestjs-typed-dynamodb": "^0.1.2",
    "nestjs-typeorm-paginate": "^2.3.2",
    "net": "^1.0.2",
    "ora": "^5.4.0",
    "os": "^0.1.1",
    "pluralize": "^8.0.0",
    "portfinder": "^1.0.28",
    "pusher-js": "^7.0.3",
    "q": "^1.5.1",
    "qs": "^6.10.1",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "stampit": "4.3.1",
    "typed-path": "^2.2.2",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2",
    "verror": "^1.10.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@commitlint/prompt-cli": "^13.1.0",
    "@digitalroute/cz-conventional-changelog-for-jira": "^6.6.0",
    "@types/agenda": "^4.1.0",
    "@types/amqplib": "^0.5.17",
    "@types/bull": "^3.15.1",
    "@types/dockerode": "^3.2.3",
    "@types/faker": "^5.5.4",
    "@types/jest": "^26.0.23",
    "@types/js-combinatorics": "^0.5.32",
    "@types/multer": "^1.4.5",
    "@types/multer-s3": "^2.7.9",
    "@types/node": "^15.0.2",
    "@types/passport-http": "^0.3.8",
    "@types/redis": "^2.8.28",
    "auto-changelog": "^2.3.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "cz-jira-smart-commit": "^3.0.0",
    "dotenv-cli": "^4.0.0",
    "husky": "^7.0.2",
    "jest": "^26.6.3",
    "jest-environment-node": "^26.6.2",
    "npm-pack-zip": "^1.2.9",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "release-it": "^14.11.6",
    "ts-jest": "^26.5.6",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.4"
  }
}
