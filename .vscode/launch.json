{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Frontend",
      "type": "node-terminal",
      "request": "launch",
      "command": "pnpm serve",
      "cwd": "${workspaceFolder}/apps/frontend",
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "/turbopack/[project]/*": "${webRoot}/*"
      }
    }
  ]
}
